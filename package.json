{
	"name": "chakra-ui-cheatsheet",
	"displayName": "chakra-ui-cheatsheet",
	"keywords": ["cheatsheet", "chakra-ui", "react"],
	"icon": "images/icon.png",
	"description": "",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:chakra-ui-cheatsheet.chakra-ui-component-props"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "chakra-ui-cheatsheet.chakra-ui-component-props",
				"title": "Chakra UI: Search <Component> Props"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
